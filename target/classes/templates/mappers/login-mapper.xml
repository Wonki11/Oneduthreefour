<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.onedumall.mapper.MembersLoginMapper">

	<select id="getLogin" resultSets="com.onedumall.dto.MembersLogin">
		SELECT * FROM Members
		WHERE members_id=#{members_id}
		AND members_pw=#{members_pw}
	</select>
</mapper>     
               
        
        