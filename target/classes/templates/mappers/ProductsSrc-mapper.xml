<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.onedumall.mapper.ProductsSrcMapper">
	<select id="searchProductSrc" parameterType="string" resultType="com.onedumall.dto.ProductsSrc">
	SELECT * FROM product
	WHERE product_name LIKE '%'||#{keyword}||'%'
	
	
	</select>

</mapper>